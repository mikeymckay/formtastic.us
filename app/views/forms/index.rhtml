<h1>Listing forms</h1>

<table>
  <tr>
  </tr>
  
<% for form in @forms %>
  <tr>
    <td><%= link_to form.name, form_url(form) %></td>
    <td><%= link_to 'Edit', edit_form_path(form) %></td>
    <td><%= link_to 'Destroy', form_path(form), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New form', new_form_path %>
<%= link_to 'Home', "/" %>
