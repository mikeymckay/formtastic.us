<%# form_for(:field_attribute_type, :url => {:action => :create}, :html => { :method => :put }) do |f| %>
<% form_for(:field_attribute_type, :url => field_attribute_type_path(@field_attribute_type), :html => { :method => :put }) do |f| %>
  <p>
    Name: <%= f.text_field :name %>
    Description: <%= f.text_area :description %>
    <%= submit_tag "Update" %>
  </p>
<% end %>
